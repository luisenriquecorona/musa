{
  "name": "musa",
  "version": "2.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "npm run cac:dev && env-cmd -f .env.server.local react-scripts start",
    "start:electron": "npm run cac:dev && env-cmd -f .env.electron.local craco start",
    "build": "npm run cac:server && env-cmd -f .env.server.prod craco build",
    "build:electron": "npm run cac:electron && env-cmd -f .env.electron.prod craco build",
    "cac:server": "node create-api-client.mjs server",
    "cac:electron": "node create-api-client.mjs electron",
    "cac:dev": "node create-api-client.mjs dev",
    "test": "npm run cac:server && craco test",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "img-palette": "^0.3.2",
    "lodash.defaultto": "^4.14.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.sortby": "^4.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "babel-plugin-macros": "^3.1.0",
    "env-cmd": "^10.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.6.2"
  }
}
