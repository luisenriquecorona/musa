{
  "name": "musa",
  "version": "3.0.1",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "env-cmd -f .env.server.local react-scripts start",
    "start:electron": "env-cmd -f .env.electron.local react-scripts start",
    "build": "env-cmd -f .env.server.prod react-scripts build",
    "build:electron": "env-cmd -f .env.electron.prod react-scripts build && git rev-parse HEAD > version",
    "test": "react-scripts test",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "lodash.defaultto": "^4.14.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.sortby": "^4.7.0",
    "quantize": "^1.0.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "babel-plugin-macros": "^3.1.0",
    "env-cmd": "^10.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "prettier": "^2.6.2"
  }
}
