{
  "name": "musa",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "BROWSER=none craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,json,md}": [
        "prettier --write",
        "git add"
      ],
      "*.scss": [
        "stylelint --quiet --fix",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@craco/craco": "^5.2.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "fuzzysort": "^1.1.4",
    "img-palette": "^0.3.2",
    "lodash-es": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-no-unsupported-browser-features": "^3.0.2"
  }
}
